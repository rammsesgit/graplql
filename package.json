{
  "name": "graplql",
  "version": "1.0.0",
  "description": "Test GraphQL with Nodejs and MongoDB",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "dev": "nodemon src/index.js --exec babel-node",
    "serve": "node dist/index.js",
    "clean": "rimraf dist",
    "start": "npm run build && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rammsesgit/graplql.git"
  },
  "keywords": [
    "graphql",
    "node",
    "mongodb"
  ],
  "author": "Isar Ramses Cadena Gaona",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rammsesgit/graplql/issues"
  },
  "homepage": "https://github.com/rammsesgit/graplql#readme",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "mongoose": "^5.7.8",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/node": "^7.7.0",
    "@babel/plugin-transform-regenerator": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "nodemon": "^1.19.4"
  }
}
